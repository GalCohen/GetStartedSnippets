PODS_ROOT = ${SRCROOT}/Pods
OTHER_LDFLAGS = -ObjC -lstdc++ -framework Accelerate -framework AudioToolbox
PODS_HEADERS_SEARCH_PATHS = ${PODS_PUBLIC_HEADERS_SEARCH_PATHS}
PODS_PUBLIC_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/Novocaine" "${PODS_ROOT}/Headers/upnpx"
HEADER_SEARCH_PATHS = ${PODS_HEADERS_SEARCH_PATHS}
PODS_BUILD_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/BuildHeaders" "${PODS_ROOT}/BuildHeaders/Novocaine" "${PODS_ROOT}/BuildHeaders/upnpx"
ALWAYS_SEARCH_USER_PATHS = YES